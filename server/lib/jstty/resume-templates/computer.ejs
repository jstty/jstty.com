{
  "columns": [
    {
      "width": <%- leftColumnWidth %>,
      "text": "Computer Tech",
      "style": "columnHeader"
    },
    {
      "width": "85%",
      "table": {
        "widths": [50, 1, "auto"],
        "body": [
          <% var keys = Object.keys(list);
             for(var i = 0; i < keys.length; i++) {
                var key = keys[i];
                var item = list[key];
          %>
          [ "<%- key %>", ":", "<%- item.join(', ') %>" ]
          <% if(i+1 != keys.length){ %>,<% } %>
          <% } %>
        ]
      },
      "margin": [0, 0, 0, 0]
      ,"layout": "noBorders"
    }
  ],
  "columnGap": <%- linePadding %>
}